<el-dialog title="收款确认" :visible="List.ModelDialog.Visible" @@close="closeDialog('List.ModelDialog')"
           :width="Common.DialogWidth"
           :close-on-click-modal="Common.CloseOnClickModal"
           :close-on-press-escape="Common.CloseOnPressEscape">
    <el-form :model="List.ModelDialog.Model" status-icon :show-message="false" ref="List.ModelDialog"
             :label-width="Common.FormLabelWidth"
             :label-suffix="Common.FormLabelSuffix">
        <h2 class="my-text-center my-danger-text">确定学员已经打款了吗?</h2>
        <el-form-item label="付款类型" prop="Enroll.PayType" required>
            <el-select v-model="List.ModelDialog.Model.Enroll.PayType" placeholder="请选择付款类型">
                <el-option :key="1" label="微信二维码支付" value="微信二维码支付"></el-option>
                <el-option :key="2" label="支付宝二维码支付" value="支付宝二维码支付"></el-option>
            </el-select>
        </el-form-item>
    </el-form>
    <div slot="footer" class="dialog-footer">
        <el-button @@click="closeDialog('List.ModelDialog')">取 消</el-button>
        <el-button type="primary" @@click="submitForm('List.ModelDialog', handleApprove)">确 定</el-button>
    </div>
</el-dialog>