<el-dialog :visible="ScoreList.ModelDialog.Visible" @@close="ScoreList.ModelDialog.Visible=false"
           :width="Common.DialogWidth"
           :close-on-click-modal="Common.CloseOnClickModal"
           :close-on-press-escape="Common.CloseOnPressEscape">
    <template slot="title">
        积分详情&nbsp;&nbsp;&nbsp;&nbsp;
        @*<el-button type="primary" size="mini" @@click="exportScoreHistoryExcel('ScoreList')"><i class="el-icon-download"></i> 导出</el-button>*@
    </template>
    <el-table :data="ScoreList.Data" border stripe
              header-row-class-name="my-main-body-table-header"
              @@sort-change="function(sort){handleSortChange(sort,'ScoreList');}"
              style="width: 100%">
        <el-table-column label="时间" prop="UpdateTime" sortable="custom">
            <template slot-scope="scope">
                <p>{{getDate(scope.row.UpdateTime)}}</p>
            </template>
        </el-table-column>
        <el-table-column label="变更" prop="Different" sortable="custom">
            <template slot-scope="scope">
                <b class="my-text-success">{{scope.row.Different>0?'+':''}}{{scope.row.Different}}</b>
            </template>
        </el-table-column>
        <el-table-column label="描述" prop="Description" sortable="custom">
            <template slot-scope="scope">
                <b class="my-text-success">{{scope.row.Description}}</b>
            </template>
        </el-table-column>
    </el-table>
    @Html.Partial("_pagination", "ScoreList")
</el-dialog>